use printf;

int fib(int n) {
    if (n == 0 || n == 1) {
        return 1;
    }
    return fib(n - 2) + fib(n - 1);
}

int main() {
    int i;
    for (i = 0; i < 20; i++)
        printf("fib(%d) = %d\n", i, fib(i));
    return 0;
}

