use printf;
use time;
use rand;
use srand;

void print(int a[], int n) {
    for (int i = 0; i < n; ++i) {
        if (i != 0)
            printf(", ");
        printf("%d", a[i]);
    }
    printf("\n");
}

int main() {
    int a[10];
    int n = 10;
    srand(time(0));
    for (int i = 0; i < n; ++i) {
        a[i] = rand() % 1000;
    }
    print(a, n);
    return 0;
}

